{
  "name": "Dark Humor",
  "version": "1.0",
  "manifest_version": 3,
  "description": "A stylish Chrome extension that displays dark humor jokes with a typing effect.",
  "action": {
    "default_popup": "popup.html",
    "default_icon": "logo.png"
  },
  "icons": {
    "128": "logo.png"
  },
  "permissions": [
    "activeTab"
  ],
  "host_permissions": [
    "https://v2.jokeapi.dev/"
  ]
}